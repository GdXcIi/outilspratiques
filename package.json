{
  "name": "outilspratiques.github.io",
  "version": "1.0.0",
  "description": "Suite d'outils web pratiques et gratuits",
  "main": "imagemin.config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "css": "postcss styles/new/main.css -o styles/dist/main.css",
    "css:watch": "postcss styles/new/main.css -o styles/dist/main.css --watch",
    "css:prod": "cross-env NODE_ENV=production postcss styles/new/main.css -o styles/dist/main.css",
    "purge": "purgecss --config purgecss.config.js",
    "css:purge": "npm run purge",
    "build:css": "npm run css:prod && npm run purge",
    "dev": "npm run css:watch"
  },
